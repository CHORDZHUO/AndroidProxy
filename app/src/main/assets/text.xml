
<config version="2.0" dns="114.114.114.114"  apn_apn="cmwap" apn_proxy="10.0.0.172" apn_port="80">

    <http host="www.chinamobile.com" port="80">
        <delate>Host,X-Online-Host,host,x-online-host</delate>
        <first-line>
            [method][tab] http://www.chinamobile.com[uri][tab] [version]\r\n
            Host: [tab][host]\r\n
        </first-line>
    </http>


    <https host="10.0.0.172" port="80"  switch="on">
        <delate>Host,X-Online-Host,host,x-online-host</delate>
        <first-line>
            [method][tab] [url][tab] [version]\r\n
            Host: [tab][host][uri]\r\n
            Accept:[tab] */*, [tab]application/vnd.wap.mms-message, [tab]application/vnd.wap.sic\r\n
            Content-Type:[tab] application/vnd.wap.mms-message\r\n
        </first-line>
    </https>

</config>
